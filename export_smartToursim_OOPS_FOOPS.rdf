     -->

    


    <!-- http://smartTourismRecommendationSystem.com#isLocatedIn -->

    <owl:ObjectProperty rdf:about="http://smartTourismRecommendationSystem.com#isLocatedIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#City"/>
        <rdfs:range rdf:resource="http://smartTourismRecommendationSystem.com#Place"/>
        <rdfs:comment>Relates a physical place such as a hotel, restaurant, or attraction to the city where it is located.</rdfs:comment>
        <rdfs:label>is Located In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smartTourismRecommendationSystem.com#operatesIn -->

    <owl:ObjectProperty rdf:about="http://smartTourismRecommendationSystem.com#operatesIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#City"/>
        <rdfs:range rdf:resource="http://smartTourismRecommendationSystem.com#Transport"/>
        <rdfs:comment>Links a public transport service to the city in which it operates.</rdfs:comment>
        <rdfs:label>operates In</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://smartTourismRecommendationSystem.com#takesPlaceIn -->

    <owl:ObjectProperty rdf:about="http://smartTourismRecommendationSystem.com#takesPlaceIn">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#City"/>
        <rdfs:range rdf:resource="http://smartTourismRecommendationSystem.com#Event"/>
        <rdfs:comment>Associates an event with the city where it is held.</rdfs:comment>
        <rdfs:label>takes Place In</rdfs:label>
    </owl:ObjectProperty>
    


