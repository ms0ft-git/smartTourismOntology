        <rdfs:label>Gastronomic Event</rdfs:label>
    </owl:Class>
    


    <!-- http://smartTourismRecommendationSystem.com#Hotel -->

    <owl:Class rdf:about="http://smartTourismRecommendationSystem.com#Hotel">
        <rdfs:subClassOf rdf:resource="http://smartTourismRecommendationSystem.com#Place"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartTourismRecommendationSystem.com#hasStars"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</xsd:minInclusive>
                            </rdf:Description>
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#int">5</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:onDataRange>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An establishment that offers lodging to travelers.</rdfs:comment>
        <rdfs:label>Hotel</rdfs:label>
    </owl:Class>
    


