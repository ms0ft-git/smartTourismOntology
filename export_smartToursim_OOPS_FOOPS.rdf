    <!-- http://smartTourismRecommendationSystem.com#EconomicTransportMadrid -->

    <owl:Class rdf:about="http://smartTourismRecommendationSystem.com#EconomicTransportMadrid">
        <rdfs:subClassOf rdf:resource="http://smartTourismRecommendationSystem.com#Transport"/>
        <rdfs:comment>Public transport service operating in Madrid with an average cost lower than 1.5.</rdfs:comment>
        <rdfs:label>Economic Transport Madrid</rdfs:label>
    </owl:Class>
    


    <!-- http://smartTourismRecommendationSystem.com#Event -->

    <owl:Class rdf:about="http://smartTourismRecommendationSystem.com#Event">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartTourismRecommendationSystem.com#takesPlaceIn"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://smartTourismRecommendationSystem.com#City"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://smartTourismRecommendationSystem.com#hasDate"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An event happening at a certain time and location, such as a concert, sport, or festival.</rdfs:comment>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


    <!-- http://smartTourismRecommendationSystem.com#GastronomicEvent -->

    <owl:Class rdf:about="http://smartTourismRecommendationSystem.com#GastronomicEvent">
