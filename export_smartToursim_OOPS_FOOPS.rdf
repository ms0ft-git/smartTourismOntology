     -->

    


    <!-- http://smartTourismRecommendationSystem.com#hasAverageCost -->

    <owl:DatatypeProperty rdf:about="http://smartTourismRecommendationSystem.com#hasAverageCost">
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Indicates the average cost of using a transport service.</rdfs:comment>
        <rdfs:label>has average cost</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smartTourismRecommendationSystem.com#hasAveragePrice -->

    <owl:DatatypeProperty rdf:about="http://smartTourismRecommendationSystem.com#hasAveragePrice">
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Refers to the average price of a meal in a restaurant.</rdfs:comment>
        <rdfs:label>has average price</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smartTourismRecommendationSystem.com#hasCompany -->

    <owl:DatatypeProperty rdf:about="http://smartTourismRecommendationSystem.com#hasCompany">
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Identifies the company that operates a transport service.</rdfs:comment>
        <rdfs:label>has company</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smartTourismRecommendationSystem.com#hasCoordinates -->

    <owl:DatatypeProperty rdf:about="http://smartTourismRecommendationSystem.com#hasCoordinates">
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#City"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Geographic coordinates (latitude and longitude) that locate a city on the map.</rdfs:comment>
        <rdfs:label>has coordinates</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smartTourismRecommendationSystem.com#hasDate -->

    <owl:DatatypeProperty rdf:about="http://smartTourismRecommendationSystem.com#hasDate">
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment>Specifies the date on which an event takes place.</rdfs:comment>
        <rdfs:label>has date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smartTourismRecommendationSystem.com#hasId -->

    <owl:DatatypeProperty rdf:about="http://smartTourismRecommendationSystem.com#hasId">
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#City"/>
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Event"/>
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Place"/>
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Identifier assigned to an entity, unique within its class (e.g., hotel, restaurant, attraction).</rdfs:comment>
        <rdfs:label>has id</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smartTourismRecommendationSystem.com#hasName -->

    <owl:DatatypeProperty rdf:about="http://smartTourismRecommendationSystem.com#hasName">
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#City"/>
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Event"/>
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Place"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The name of the entity, such as a hotel, city, or attraction.</rdfs:comment>
        <rdfs:label>has name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smartTourismRecommendationSystem.com#hasPricePerNight -->

    <owl:DatatypeProperty rdf:about="http://smartTourismRecommendationSystem.com#hasPricePerNight">
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Price charged for one night’s stay in a hotel.</rdfs:comment>
        <rdfs:label>has price per night</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smartTourismRecommendationSystem.com#hasRating -->

    <owl:DatatypeProperty rdf:about="http://smartTourismRecommendationSystem.com#hasRating">
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Place"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Numerical score representing the quality or user evaluation of a service or place.</rdfs:comment>
        <rdfs:label>has rating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://smartTourismRecommendationSystem.com#hasStars -->

    <owl:DatatypeProperty rdf:about="http://smartTourismRecommendationSystem.com#hasStars">
        <rdfs:domain rdf:resource="http://smartTourismRecommendationSystem.com#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>Star rating assigned to a hotel based on its category or services.</rdfs:comment>
        <rdfs:label>has stars</rdfs:label>
    </owl:DatatypeProperty>
    


